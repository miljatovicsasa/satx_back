    },
    "email": {
      "type": "string",
      "required": true
    },
    "message": {
      "type": "blocks",
      "required": true
    },
    "watch": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::watch.watch"
    }
  }
}
