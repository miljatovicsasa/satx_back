{
  "kind": "collectionType",
  "collectionName": "contact_requests",
  "info": {
    "singularName": "contact-request",
    "pluralName": "contact-requests",
    "displayName": "Purchase"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "message": {
      "type": "blocks",
      "required": true
    },
    "watch": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::watch.watch"
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "quantity": {
      "type": "integer",
      "required": true
    },
    "priceSnapshot": {
      "type": "decimal",
      "required": true
    },
    "currency": {
      "type": "enumeration",
      "enum": [
        "EUR",
        "RSD"
      ]
    },
    "statusOfPurchase": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "new",
        "confirmed",
        "awaiting_payment",
        "paid",
        "shipped",
        "cancelled"
      ]
    },
    "internalNote": {
      "type": "text"
    },
    "country": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "prefferedContact": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "phoneCall",
        "phoneMessage",
        "whatsApp",
        "Viber",
        "email"
      ]
    }
  }
}
