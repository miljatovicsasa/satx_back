{
  "kind": "collectionType",
  "collectionName": "watches",
  "info": {
    "singularName": "watch",
    "pluralName": "watches",
    "displayName": "Watch"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "shortDescription": {
      "type": "text"
    },
    "description": {
      "type": "blocks"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "currency": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "eur",
        "rsd"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "inStock": {
      "type": "boolean",
      "default": true
    },
    "condition": {
      "type": "enumeration",
      "enum": [
        "new",
        "pre-owned"
      ]
    },
    "gender": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "men",
        "women",
        "unisex"
      ]
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "watches"
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "mappedBy": "watches"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "watches"
    }
  }
}
